t={}
for j in range(1,4*7+1):
        t[j]=-1
def build(a,v,start,end):
    n=len(a)
    if start==end:
        t[v]=a[start]
    else:
        mid=(start+end)/2
        build(a,v*2,start,mid)
        build(a,v*2+1,mid+1,end)
        t[v]=min(t[2*v],t[2*v+1])
    return t
print build([18,17,13,19,15,11,20],1,0,6)
